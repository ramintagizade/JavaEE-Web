
public class App {

	public static void main(String[] args) {
		//Composition
		ConsoleLogWriter logWriter = new ConsoleLogWriter();
		Logger logger = new Logger(logWriter);
		logger.write("hello there");
		//Inheritance 
		ConsoleLogWriter2 logWriter = new ConsoleLogWriter2();
		Logger logger = new Logger(logWriter);
		logger.write("hello there");
	}

}
