import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.PrintWriter;

public class B {

  public static void main(String [] args) throws NumberFormatException, IOException{
    BufferedReader inp = new BufferedReader (new FileReader("B-small-practice.in"));
    Long t = Long.parseLong(inp.readLine());
    int id = 1;
    Long T = t;
    PrintWriter writer = new PrintWriter("output.out", "UTF-8");
    while(id<=t){
      Long n = Long.parseLong(inp.readLine());
      writer.println("Case #"+id+": "+ solveBsmall(n));
     
	  if(T==id)
		writer.close();
      id++;
    }
  }
  public static Long solveBlarge(Long n){
	  String s = String.valueOf(n);
	  String cur = "";
	  int reduce = 0;
	  int prev = 0;
	  int max = 9;
	  for(int i=0;i<s.length();i++){
		 if(s.charAt(i) <= s.charAt(i+1)){
			 cur+=s.charAt(i);
			 continue;
		 }
		 else {
			 cur+=s.charAt(i)-'0'-1;
		 }
	  }
	  return (long) 1000;
  }
  public static Long solveBsmall(Long n){
	  while(n>0 ){
		  if(isTidy(n)) {
			 break;
		  }
		  n--;
	  }
	  return n;
  }
  public static boolean isTidy(Long t){
	  Long n = t;
	  Long previous =  n%10;
	  boolean ok = true;
	  while(n>0){
		  Long last =  n%10;
		  if(previous<last){
			  ok = false;
			  break;
		  }
		  previous = last;
		  n = n/10;
	  }
	  if(ok) return true;
	  return false;
  }
}
